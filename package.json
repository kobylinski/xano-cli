{
  "name": "@deligopl/xano-cli",
  "description": "CLI for syncing XanoScript files with Xano backend",
  "version": "1.1.0",
  "author": "Deligo <dev@deligo.pl>",
  "bin": {
    "xano": "./bin/run.js"
  },
  "bugs": "https://github.com/kobylinski/xano-cli/issues",
  "dependencies": {
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "inquirer": "^8.2.5",
    "js-yaml": "^4.1.0",
    "xanoscript-lint": "^0.0.3"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/inquirer": "^8.2.5",
    "@types/js-yaml": "^4.0.9",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "baseline-browser-mapping": "^2.9.14",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./.claude",
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/kobylinski/xano-cli",
  "keywords": [
    "xano",
    "xanoscript",
    "cli",
    "sync",
    "backend"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "xano",
    "dirname": "xano",
    "commands": "./dist/commands",
    "helpClass": "./dist/help",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "kobylinski/xano-cli",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "dev": "./bin/dev.js",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/lib/**/*.test.ts\"",
    "test:integration": "XANO_INTEGRATION_TEST=true mocha --forbid-only \"test/integration/**/*.test.ts\"",
    "test:all": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
